<?xml version="1.0" encoding="UTF-8"?>

<behavior name="Drive Car">

	<executable package_path="racecar_flexbe_behaviors.drive_car_sm" class="DriveCarSM" />
	<tagstring>Drive</tagstring>
	<author>Earl</author>
	<date>Thu Oct 31 2019</date>
	<description>
		It enables us to drive the car
	</description>


	<!-- Contained Behaviors -->

	<!-- Available Parameters -->
	<params>

		<param type="numeric" name="my_speed" default="0.4" label="my_speed" hint="Sets the my_speed">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="my_travel_dist" default="20" label="my_travel_dist" hint="Sets the my_travel_dist">
			<min value="0" />
			<max value="50" />
		</param>

		<param type="numeric" name="my_obstacle_dist" default="1" label="my_obstacle_dist" hint="Sets the my_obstacle_dist">
			<min value="0" />
			<max value="6" />
		</param>

		<param type="numeric" name="my_t_speed" default="0.1" label="my_t_speed" hint="Sets the my_t_speed">
			<min value="0" />
			<max value="1" />
		</param>

		<param type="numeric" name="my_turn_angle" default="-0.3" label="my_turn_angle" hint="Sets the my_turn_angle">
			<min value="-1" />
			<max value="1" />
		</param>

		<param type="numeric" name="my_forward_dist" default="2" label="my_forward_dist" hint="Sets the my_forward_dist">
			<min value="0" />
			<max value="3" />
		</param>

		<param type="numeric" name="my_timeout" default="10" label="my_timeout" hint="Sets the my_timeout">
			<min value="0" />
			<max value="10" />
		</param>

	</params>


</behavior>